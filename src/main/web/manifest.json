{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "js": ["kotlin.js", "crawl-extension-2.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],

  "permissions": [
    "activeTab",
    "storage"
  ]
}
